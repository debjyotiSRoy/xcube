accelerate launch --mixed_precision=fp16 --num_processes=1 \
                                    ../train.py \
                                    --data="mimic3-9k_18000" \
                                    --fp16 \
                                    --workers=16 \
                                    --track_train \
                                    --log \
                                    --lm \
                                    --epochs="[10, 1, 1, 1, 4]" \
                                    --lrs="[(3e-2,1e-3), (1e-2,1e-3), (1e-2, 1e-3), (1e-2,1e-3), (1e-6,1e-6)]" \
                                    --bs=16 \
                                    --save_model \
                                    --fname="mimic3-9k_clas_18000_plant" \
                                    --runs=1 \
                                    --plant \
                                    --attn_init="(0,1,1)" \
                                    --infer=0 \
                                    --metrics="partial(precision_at_k, k=1); F1ScoreMulti(thresh=0.5, average='macro'); F1ScoreMulti(thresh=0.5, average='micro')"
echo "files created:"
find .. -name "mimic3-9k_clas_18000_plant*" 
